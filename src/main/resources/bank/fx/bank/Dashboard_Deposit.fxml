<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="510.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/19"
   xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="bank.fx.bank.Controller.DashbaordDepositController">
   <children>
      <Pane layoutX="179.0" layoutY="20.0" prefHeight="465.0" prefWidth="593.0">
         <children>
            <Button alignment="CENTER" layoutX="59.0" layoutY="22.0" mnemonicParsing="false"
               prefHeight="52.0" prefWidth="476.0" styleClass="deposit"
               stylesheets="@CSS/dashbaord.css" text="Deposit">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Pane layoutX="67.0" layoutY="104.0" prefHeight="102.0" prefWidth="200.0">
               <children>
                  <Label layoutX="24.0" layoutY="14.0" prefHeight="17.0" prefWidth="48.0"
                     text="User ID :" />
                  <TextField fx:id="userIdBtn" layoutX="78.0" layoutY="10.0" prefHeight="25.0"
                     prefWidth="101.0" />
                  <Label layoutX="24.0" layoutY="61.0" text="Date :" />
                  <DatePicker fx:id="DateBtn" layoutX="78.0" layoutY="57.0" prefHeight="25.0"
                     prefWidth="101.0" promptText="Date">
                     <effect>
                        <Glow />
                     </effect>
                  </DatePicker>
               </children>
            </Pane>
            <Pane layoutX="112.0" layoutY="178.0" prefHeight="287.0" prefWidth="453.0">
               <children>
                  <TextField fx:id="accountNumberBtn" layoutX="212.0" layoutY="76.0"
                     prefHeight="25.0" prefWidth="151.0" />
                  <TextField fx:id="depositIdBtn" layoutX="212.0" layoutY="119.0" prefHeight="25.0"
                     prefWidth="151.0" />
                  <TextField fx:id="accountBtn" alignment="CENTER" layoutX="209.0" layoutY="164.0"
                     prefHeight="25.0" prefWidth="157.0" />
                  <Button fx:id="DepositBtn" layoutX="96.0" layoutY="220.0" mnemonicParsing="false"
                     onAction="#depositBtn" prefHeight="32.0" prefWidth="273.0"
                     styleClass="submit-btn" stylesheets="@CSS/signup.css" text="Deposit"
                     textFill="WHITE" />
                  <Label layoutX="96.0" layoutY="123.0" text="Deposit ID :" />
                  <Label layoutX="97.0" layoutY="80.0" text="Account Number :" />
                  <Label layoutX="96.0" layoutY="168.0" text="Amount :" />
                  <Label fx:id="errorBtn" layoutX="20.0" layoutY="21.0" prefHeight="17.0"
                     prefWidth="124.0" textFill="#ed4848" />
                  <Label fx:id="errorBtn1" layoutX="114.0" layoutY="211.0" prefHeight="17.0"
                     prefWidth="334.0" textFill="#e03434" />
               </children>
            </Pane>
            <ImageView fitHeight="157.0" fitWidth="258.0" layoutX="297.0" layoutY="77.0"
               pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/card.png" />
               </image>
            </ImageView>
         </children>
         <effect>
            <Glow />
         </effect>
      </Pane>
   </children>
</AnchorPane>