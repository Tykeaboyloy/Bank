   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="330.0" layoutY="90.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/logo.png" />
         </image>
      </ImageView>
      <Button layoutX="374.0" layoutY="238.0" mnemonicParsing="false" onAction="#switchToRegister" prefHeight="24.0" prefWidth="112.0" text="Register" />
      <Button layoutX="374.0" layoutY="185.0" mnemonicParsing="false" onAction="#switchToLogin" prefHeight="24.0" prefWidth="112.0" text="Login" />
      <Button layoutX="374.0" layoutY="290.0" mnemonicParsing="false" onAction="#exit" prefHeight="24.0" prefWidth="112.0" text="Exit" />
   </children>
</AnchorPane>
