   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="200.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/logo.png" />
         </image>
      </ImageView>
      <Button layoutX="244.0" layoutY="239.0" mnemonicParsing="false" onAction="#switchToRegister" prefHeight="24.0" prefWidth="112.0" text="Register" />
      <Button layoutX="244.0" layoutY="188.0" mnemonicParsing="false" onAction="#switchToLogin" prefHeight="24.0" prefWidth="112.0" text="Login" />
      <Button layoutX="244.0" layoutY="291.0" mnemonicParsing="false" onAction="#exit" prefHeight="24.0" prefWidth="112.0" text="Exit" />
   </children>
</AnchorPane>
